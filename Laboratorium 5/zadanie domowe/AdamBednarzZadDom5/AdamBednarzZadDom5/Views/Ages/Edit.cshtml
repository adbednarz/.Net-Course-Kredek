@model Age

@using (Html.BeginForm("Edit", "Ages", FormMethod.Post))
{
    <h3>Edytuj epokę</h3>
    <hr />

    <div class="form-group">
        @Html.LabelFor(model => model.Name)
        <div class="col-md">
            @Html.TextBoxFor(model => model.Name)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.YearOfStart)
        <div class="col-md">
            @Html.TextBoxFor(model => model.YearOfStart)
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.YearOfEnd)
        <div class="col-md">
            @Html.TextBoxFor(model => model.YearOfEnd)
        </div>
    </div>

    <input type="submit" value="Edytuj" class="btn btn-primary" />
}